@model WebViewModels.UserViewModel
@{
    ViewData["Title"] = "UserProfilePage";
}

<h2>UserProfilePage</h2>


<div class="row">
    <div class="col-md-3 col-md-offset-1">
        <h2 class="col-md-4">@Model.Firstname</h2>
        <h2 class="col-md-4">@Model.Lastname</h2>
    </div>
    <div class="col-md-3 col-md-offset-1">
        <h2 class="col-md-4">@Model.Email</h2>
        <h2 class="col-md-4">@Model.Country</h2>
        <h2 class="col-md-4">@Model.City</h2>
    </div>
    <div class="col-md-3 col-md-offset-1">
        <h2 class="col-md-4">@Model.Address</h2>
        <h2 class="col-md-4">@Model.DateOfBirth</h2>
    </div>
</div>

